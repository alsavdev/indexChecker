{
  "name": "checker-index",
  "productName": "Checker Index",
  "version": "1.0.1",
  "description": "Using for check index",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "npx electron-builder build",
    "deploy": "electron-builder -p always"
  },
  "author": "Alsav System",
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.7",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "puppeteer": "^21.0.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.3.0",
    "@electron-forge/maker-deb": "^6.3.0",
    "@electron-forge/maker-rpm": "^6.3.0",
    "@electron-forge/maker-squirrel": "^6.3.0",
    "@electron-forge/maker-zip": "^6.3.0",
    "@electron-forge/plugin-auto-unpack-natives": "^6.3.0",
    "electron": "^25.9.8",
    "electron-builder": "^24.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alsavdev/indexChecker"
  },
  "build": {
    "appId": "Alsav Checker",
    "productName": "Alsav Checker",
    "compression": "maximum",
    "copyright": "Copyrights 2024 Alsav System",
    "publish": {
      "provider": "github",
      "owner": "alsavdev",
      "repo": "indexChecker",
      "private": true
    },
    "win": {
      "target": "nsis",
      "icon": "assets/logos.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "extraFiles": [
      "src/extension/spoof",
      "src/extension/captcha",
      "src/extension/cghost",
      "src/data/"
    ]
  }
}
